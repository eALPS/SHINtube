<!DOCTYPE html>
<html lang="jp">
  <head>
    <title>SHINtube</title>
    <link rel='stylesheet' href='./stylesheets/common.css'/>
    <link rel="icon" href="./images/favicon.ico">
    <%- include('./common/theme') %>
    <%- include('./common/params') %>
    <link rel='stylesheet' href='./stylesheets/listbase.css'/>
  </head>
  <body class="theme-body">
    <%- include('./common/header') %>
    <div id="contents">
      <%- include('./common/side-menu',{'view_type':view_type,'role':role}) %>
      <% if (view_type === 'service_class') { %>
        <%- include('./common/adminlist') %>
      <% } else { %>
        <%- include('./common/videolist') %>
      <% } %>
      <% if (role === 'admin') { %>
        <%- include('./common/copy-overlay') %>
      <% } %>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"></script>
  </body>

</html>